<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.uiexamples">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.UIExamples">
        <activity android:name=".GetFlights"
            android:theme="@style/Theme.UIExamples.NoActionBar"></activity>
        <activity
            android:name=".CrudSeats"
            android:theme="@style/Theme.UIExamples.NoActionBar" />
        <activity
            android:name=".RegisterActivity"
            android:theme="@style/Theme.UIExamples.NoActionBar" />
        <activity
            android:name=".CrudTickets"
            android:theme="@style/Theme.UIExamples.NoActionBar" />
        <activity
            android:name=".CrudFlights"
            android:theme="@style/Theme.UIExamples.NoActionBar" />
        <activity
            android:name=".TicketEditActivity"
            android:theme="@style/Theme.UIExamples.NoActionBar" />
        <activity
            android:name=".MenuExample"
            android:label="@string/title_activity_menu_example"
            android:theme="@style/Theme.UIExamples.NoActionBar" />
        <activity
            android:name=".LoginExample"
            android:theme="@style/Theme.UIExamples.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity" />

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>